<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Airline</th>
      <th>Airport</th>
      <th>Flight No.</th>
      <th>Departure</th>
    </tr>
  </thead>
  <tbody>
    <% flights.forEach(function(f){%>
    <tr>
      <td><%=f.airline %></td>
      <td><%=f.airport %></td>
      <td><%=f.flightNo %></td>
      <td><%=f.departs %></td>
    </tr>
    <% }); %>
  </tbody>
</table>

<%- include('../partials/footer') %>
